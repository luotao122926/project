<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>registerServlet</servlet-name>
        <servlet-class>servlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registerServlet</servlet-name>
        <url-pattern>/registerServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>goodsinfoLoadDataServlet</servlet-name>
        <servlet-class>servlet.GoodsinfoLoadDataServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>goodsinfoLoadDataServlet</servlet-name>
        <url-pattern>/goodsinfoLoadDataServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>goodsinfoUpdateServlet</servlet-name>
        <servlet-class>servlet.GoodsinfoUpdateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>goodsinfoUpdateServlet</servlet-name>
        <url-pattern>/goodsinfoUpdateServlet</url-pattern>
    </servlet-mapping>

    <servlet>
    <servlet-name>goodsinfoQueryServlet</servlet-name>
    <servlet-class>servlet.GoodsinfoQueryServlet</servlet-class>
</servlet>
    <servlet-mapping>
        <servlet-name>goodsinfoQueryServlet</servlet-name>
        <url-pattern>/goodsinfoQueryServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>goodsinfoInsertServlet</servlet-name>
        <servlet-class>servlet.GoodsinfoInsertServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>goodsinfoInsertServlet</servlet-name>
        <url-pattern>/goodsinfoInsertServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>goodsinfoDeleteServlet</servlet-name>
        <servlet-class>servlet.GoodsinfoDeleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>goodsinfoDeleteServlet</servlet-name>
        <url-pattern>/goodsinfoDeleteServlet</url-pattern>
    </servlet-mapping>

</web-app>